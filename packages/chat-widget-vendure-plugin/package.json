{
    "name": "@connectycube/chat-widget-vendure-plugin",
    "description": "A chat widget Vendure plugin",
    "version": "0.10.0",
    "license": "Apache 2.0",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist/**/*",
        "CHANGELOG.md",
        "docs"
    ],
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "dev": "ts-node dev-server/index.ts",
        "codegen": "ts-node generate-types.ts",
        "build": "rimraf dist && tsc -p ./tsconfig.build.json && ts-node copy-ui-src.ts",
        "e2e": "cross-env PACKAGE=example-plugin vitest -c ../../utils/e2e/vitest.config.mts"
    },
    "dependencies": {
        "@connectycube/chat-widget": "^0.26.0"
    }
}
